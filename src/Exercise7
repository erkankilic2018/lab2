#include "eecs230.h"

//
// Lab 2 programs
//
int main() {
    int a= 0;
    int b= 1;
    int c= 2;



    cout<<"Write four integers: \n";
    cin>>a>>b>>c;

    int max = a;

    if (max<b) {

        max= b;
    }
    if (max<c) {

        max = c;
    }

    int min = a;

    if (b<min) {

        min = b;
    }
    if (c<min) {

        min= c;
    }

    int middle= a;

    if (middle<c and middle>a){

        middle= b;

    }

    if (middle<a and middle>b){

        middle= c;

    }

    cout<<min<<middle<< max;




}
